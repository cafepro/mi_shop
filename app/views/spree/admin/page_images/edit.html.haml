- content_for :page_title do
  = Spree.t(:editing_page_image)
%div{"data-hook" => "admin_page_image_edit_form_header"}
  = render 'spree/shared/error_messages', target: @page_image
%div{"data-hook" => "admin_page_image_edit_form"}
  = form_with model: [:admin, @page_image], local: true do |f|
    .row
      .col-md-12
        = render 'form', f: f
        = render 'spree/admin/shared/edit_resource_links'
